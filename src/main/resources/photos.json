{
  "index_patterns": [ "photos_*" ],
  "mappings":{
    "properties":{
      "archived":{
        "type":"boolean"
      },
      "creationTime":{
        "type":"date"
      },
      "description":{
        "type":"text"
      },
      "geoData":{
        "type":"geo_point"
      },
      "geoDataExif":{
        "type":"geo_point"
      },
      "imageViews":{
        "type":"short"
      },
      "modificationTime":{
        "type":"date"
      },
      "photoTakenTime":{
        "type":"date"
      },
      "title":{
        "type":"text",
        "fields":{
          "keyword":{
            "type":"keyword",
            "ignore_above":256
          }
        }
      },
      "trashed":{
        "type":"boolean"
      }
    }
  }
}